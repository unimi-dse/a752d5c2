install.packages("ggplot2")
install.packages("ggpubr")
library(ggplot2)

#ROME
rome<-read.csv("C:\\Users\\Notebook\\Desktop\\pm10_roma.csv", header=TRUE)
rome
date_rome <- as.Date(rome$utc, format("%d/%m/%y"))
r_date<-c(date_rome)
r_value<-c(rome$value)
dfr <- data.frame(r_date, r_value)
dfr

r_graph <- ggplot(dfr, aes(r_date, r_value))+geom_line()
r_graph
r_boxplot<-ggplot(dfr,aes(,r_value))+geom_boxplot()
r_boxplot

r_mean<- round(mean(r_value),0)
r_mean
r_sd <- round(sd(r_value),0)
r_sd
r_max<-max(r_value)
r_max
r_min<-round(min(r_value),0)
r_min

sprintf("Mean value pm10 = %d; Standard Error Value= %d; Max Value= %d; Min Value= %d",r_mean,r_sd,r_max,r_min)

